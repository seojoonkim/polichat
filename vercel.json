{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "functions": {
    "api/chat.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/eval.js": {
      "maxDuration": 30,
      "memory": 512
    }
  }
}
