{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/:path*" },
    { "source": "/((?!api/).*)", "destination": "/index.html" }
  ],
  "functions": {
    "api/chat.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/eval.js": {
      "maxDuration": 30,
      "memory": 512
    }
  }
}
